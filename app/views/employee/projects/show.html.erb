<h1 class = "text-center">Project</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Project Name</th>
      <th scope="col">Description</th>
      <th scope="col">Deadline</th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th><%= @project.project_name %></th>
      <td><%= @project.project_description %></td>
      <td><%= @project.project_deadline %></td>
    </tr>
  </tbody>
</table>
<div class="border border-dark">
  <h3>Assigned to</h3>
  <ul>
  <% @assigned_employees.each do |employee|%>
  <li><b><%=employee.name%></b></li>
  <% end%>
  </ul> 
  
</div>